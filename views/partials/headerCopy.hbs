{{#if isHidden}}
    <div class="header"></div>
{{else}}
    <div class="header header--dubbed">
        <div class="header__port">
            <div class="header__logo">
                <div class="header__block"></div>
                <div class="header__slogan">Юрист-на-Дону</div>
            </div>
            <div class="header__menu-humb-box">
                <div class="header__menu-humb-list" id="btnHumb"></div>
                <div class="header__menu-humb-dropdown-box" id="dropdownHumb">
                    <ul class="header__menu-humb">
                        {{#if isIndex}}
                            <li class="header__item-humb is-active">
                                <a href="#" class="header__humb-link">Главная</a>
                            </li>
                        {{else}}
                            <li class="header__item-humb">
                                <a href="#" class="header__humb-link">Главная</a>
                            </li>
                        {{/if}}
                        {{#if isAbout}}
                            <li class="header__item-humb is-active">
                                <a href="#" class="header__humb-link">О проекте</a>
                            </li>
                        {{else}}
                            <li class="header__item-humb">
                                <a href="#" class="header__humb-link">О проекте</a>
                            </li>
                        {{/if}}
                        <li class="header__item-humb">
                            <a href="#" class="header__humb-link">Услуги компании</a>
                        </li>
                        {{#if isContact}}
                            <li class="header__item-humb is-active">
                                <a href="#" class="header__humb-link">Контакты</a>
                            </li>
                        {{else}}
                            <li class="header__item-humb">
                                <a href="#" class="header__humb-link">Контакты</a>
                            </li>
                        {{/if}}
                        
                    </ul>
                </div>
            </div>
            <div class="header__nav-box">
                <nav class="navbar navbar-expand-lg header__nav">
                    <div class="container-fluid header__menu-box">
                        {{> menu}}
                    </div>
                </nav>
            </div>
            <div class="header__support">
                <div class="header__call-btn"></div>
                <div class="header__phone-number">{{phone}}</div>
            </div>
        </div>
    </div>
{{/if}}